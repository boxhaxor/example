{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Spring": {
    "Application": {
      "Name": "home"
    },
    "Cloud": {
      "Config": {
        "HealthEnabled": true,
        "DiscoveryEnabled": true,
        "Uri": "http://localhost:8888",
        "ValidateCertificates": false
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://localhost:8881"
}
